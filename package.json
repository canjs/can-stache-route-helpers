{
  "name": "can-stache-route-helpers",
  "version": "1.1.3",
  "description": "stache helpers for can-route",
  "homepage": "https://canjs.com/doc/can-stache-route-helpers.html",
  "repository": {
    "type": "git",
    "url": "git://github.com/canjs/can-stache-route-helpers.git"
  },
  "author": {
    "name": "DoneJS Core",
    "email": "core@donejs.com",
    "url": "donejs.com"
  },
  "scripts": {
    "preversion": "npm test",
    "postpublish": "git push --tags && git push",
    "testee": "testee test.html --browsers firefox",
    "test": "npm run jshint && npm run testee",
    "jshint": "jshint ./*.js --config",
    "release:patch": "npm version patch && npm publish",
    "release:minor": "npm version minor && npm publish",
    "release:major": "npm version major && npm publish"
  },
  "main": "can-stache-route-helpers",
  "keywords": [
    "canjs",
    "donejs",
    "donejs-plugin"
  ],
  "steal": {
    "npmIgnore": [
      "testee",
      "generator-donejs",
      "donejs-cli",
      "steal-tools"
    ]
  },
  "dependencies": {
    "can-reflect": "^1.16.3",
    "can-route": "^4.0.0",
    "can-stache": "^4.0.0"
  },
  "devDependencies": {
    "can-map": "^4.1.1",
    "can-route-mock": "^1.0.0",
    "jshint": "^2.9.1",
    "steal": "^2.1.8",
    "steal-qunit": "^1.0.1",
    "steal-tools": "^2.0.2",
    "testee": "^0.8.0"
  },
  "license": "MIT"
}
