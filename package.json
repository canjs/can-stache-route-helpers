{
  "name": "can-stache-route-helpers",
  "version": "0.0.0",
  "description": "helpers for can-route",
  "homepage": "http://canjs.com",
  "repository": {
    "type": "git",
    "url": "git://github.com/donejs/can-stache-route-helpers.git"
  },
  "author": {
    "name": "DoneJS Core",
    "email": "core@donejs.com",
    "url": "donejs.com"
  },
  "scripts": {
    "preversion": "npm test",
    "postpublish": "git push --tags && git push",
    "testee": "testee test.html --browsers firefox",
    "test": "npm run jshint && npm run testee",
    "jshint": "jshint ./*.js --config",
    "release:patch": "npm version patch && npm publish",
    "release:minor": "npm version minor && npm publish",
    "release:major": "npm version major && npm publish",
    "develop": "done-serve --static --develop --port 8080"
  },
  "main": "can-stache-route-helpers",
  "browser": {
    "transform": [
      "cssify"
    ]
  },
  "browserify": {
    "transform": [
      "cssify"
    ]
  },
  "keywords": [
    "canjs",
    "donejs",
    "donejs-plugin"
  ],
  "steal": {
    "configDependencies": [
      "live-reload"
    ],
    "npmIgnore": [
      "testee",
      "generator-donejs",
      "donejs-cli",
      "steal-tools"
    ],
    "plugins": [
      "steal-less",
      "steal-stache"
    ]
  },
  "dependencies": {
    "can-route": "^3.2.3",
    "can-stache": "^3.0.20",
    "can-view-autorender": "^3.0.4"
  },
  "devDependencies": {
    "can-map": "^3.4.0",
    "jshint": "^2.9.1",
    "steal": "^1.3.1",
    "steal-qunit": "^1.0.1",
    "steal-tools": "^1.2.0",
    "testee": "^0.3.0"
  },
  "license": "MIT"
}
